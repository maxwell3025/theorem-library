{
    "users": [
        {
            "name": "guest",
            "password": "guest",
            "tags": "administrator"
        }
    ],
    "vhosts": [
        {
            "name": "/"
        }
    ],
    "permissions": [
        {
            "user": "guest",
            "vhost": "/",
            "configure": ".*",
            "write": ".*",
            "read": ".*"
        }
    ],
    "policies": [
        {
            "vhost": "/",
            "name": "max-queue-length",
            "pattern": ".*",
            "apply-to": "queues",
            "definition": {
                "max-length": 3,
                "overflow": "reject-publish"
            }
        }
    ]
}